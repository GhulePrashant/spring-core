<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns:context="http://www.springframework.org/schema/context"
	xmlns:mvc="http://www.springframework.org/schema/mvc"
	xsi:schemaLocation="http://www.springframework.org/schema/beans
	http://www.springframework.org/schema/beans/spring-beans.xsd
	http://www.springframework.org/schema/context
	http://www.springframework.org/schema/context/spring-context.xsd
	http://www.springframework.org/schema/mvc
	http://www.springframework.org/schema/mvc/spring-mvc.xsd">

	<bean name="emp1" class="com.springcore.collections.Employee">
		<property name="empName" value="Prashant" />

		<property name="phones">
			<list>
				<value>9923644790</value>
				<value>9579313440</value>
			</list>
		</property>

		<property name="addresses">
			<set>
				<value>Pune</value>
				<value>Osmanabad</value>
				<value>Beed</value>
			</set>
		</property>

		<property name="courses">
			<map>
				<entry key="Java" value="3 Months" />
				<entry key="Web Dev" value="2 Months" />
			</map>
		</property>
	</bean>

</beans>